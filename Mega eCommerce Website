<script type="text/javascript">
    $('[data-toggle="tabajax"]').click(function (e) {
        var $this = $(this),
                loadurl = $this.attr('href'),
                targ = $this.attr('data-target');
        console.log(targ);
        if ($(targ).is(":empty")) {
            $(targ).html("<div class=\"busy-ajax\"><img src=\"images/ajax-loader3.gif\" /></div>");

            $.get(loadurl, function (data) {
                $(targ).html(data);
            });

        }
        $this.tab('show');
        return false;
    });

</script>

